{
  "name": "DavidTesting",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "ipods": "cd ios && rm -rfv Pods/ Podfile.lock build && pod install",
    "ios:dev": "ENVFILE=.env.dev npx react-native run-ios --mode=Debug --scheme \"dev\"",
    "ios:dev-release": "ENVFILE=.env.dev npx react-native run-ios --mode=Release --scheme \"dev\"",
    "ios:staging": "ENVFILE=.env.staging npx react-native run-ios --mode=Debug --scheme \"staging\"",
    "ios:staging-release": "ENVFILE=.env.staging npx react-native run-ios --mode=Release --scheme \"staging\"",
    "ios:prod": "ENVFILE=.env.prod npx react-native run-ios --mode=Debug --scheme \"prod\"",
    "ios:prod-release": "ENVFILE=.env.prod npx react-native run-ios --mode=Release --scheme \"prod\"",
    "android:dev": "ENVFILE=.env.dev npx react-native run-android --mode=devDebug",
    "android:dev-release": "ENVFILE=.env.dev npx react-native run-android --mode=devRelease",
    "android:staging": "ENVFILE=.env.staging npx react-native run-android --mode=stagingDebug",
    "android:staging-release": "ENVFILE=.env.staging npx react-native run-android --mode=stagingRelease",
    "android:prod": "ENVFILE=.env.prod npx react-native run-android --mode=prodDebug",
    "android:prod-release": "ENVFILE=.env.prod npx react-native run-android --mode=prodRelease",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@react-native-firebase/app": "^18.6.0",
    "@react-native-firebase/auth": "^18.6.1",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "react": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-native": "0.72.6",
    "react-native-config": "^1.5.1",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-splash-screen": "^3.3.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
